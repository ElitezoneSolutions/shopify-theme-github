ding: 0;\n    transition: opacity 0.3s ease-in-out, visibility 0.3s ease-in-out;\n  }\n\n  .sticky-bar.visible {\n    opacity: 1;\n    visibility: visible;\n  }\n\n  .sticky-bar-container {\n    max-width: 1200px;\n    margin: 0 auto;\n    display: flex;\n    align-items: center;\n    justify-content: space-between;\n    padding: 0 16px;\n    height: 100%;\n  }\n\n  .sticky-bar-left {\n    display: flex;\n    flex-direction: row;\n    align-items: center;\n    height: 100%;\n    flex: 1;\n    min-width: 0;\n  }\n\n  .sticky-bar-details {\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    flex: 1;\n    min-width: 0;\n    margin-left: 12px;\n  }\n\n  .sticky-bar-details .title {\n    font-weight: bold;\n    font-size: 14px;\n    margin: 0;\n    line-height: 1.2;\n    overflow: hidden;\n    text-overflow: ellipsis;\n    white-space: nowrap;\n    color: rgb(var(--color-foreground));\n  }\n\n  .sticky-bar-details .price-section {\n    display: flex;\n    align-items: center;\n    margin-top: 2px;\n  }\n\n  .sticky-bar-details .price {\n    color: #e60023;\n    font-weight: bold;\n    font-size: 12px;\n  }\n\n  .sticky-bar-details .compare-price {\n    text-decoration: line-through;\n    color: #888;\n    margin-left: 8px;\n    font-size: 11px;\n  }\n\n  .add-to-cart-btn {\n    padding: 6px 16px;\n    background-color: rgba(var(--color-button), var(--alpha-button-background));\n    color: rgb(var(--color-button-text));\n    border: none;\n    border-radius: 0px;\n    cursor: pointer;\n    text-decoration: none;\n    font-size: 14px;\n    height: 36px;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    min-width: 120px;\n  }\n\n  .add-to-cart-btn:hover {\n    background-color: rgba(var(--color-button), 0.9);\n  }\n\n  .product-image {\n    display: block;\n    width: 40px;\n    height: 40px;\n  }\n\n  @media (max-width: 768px) {\n    .sticky-bar-container {\n      flex-direction: row;\n      justify-content: space-between;\n    }\n\n    .sticky-bar-left {\n      display: flex;\n      flex-direction: row;\n      justify-content: flex-start;\n      margin-right: 12px;\n    }\n\n    .sticky-bar-details .title {\n      font-size: 12px;\n      max-width: 150px;\n    }\n\n    .sticky-bar-details .title-short {\n      display: block;\n    }\n\n    .sticky-bar-details .title-full {\n      display: none;\n    }\n\n    .add-to-cart-btn {\n      min-width: 120px;\n      margin-top: 0;\n      height: 40px;\n    }\n\n    .product-image {\n      display: none;\n    }\n  }\n\n  @media (min-width: 769px) {\n    .product-image {\n      display: block;\n    }\n\n    .sticky-bar-details .title-short {\n      display: none;\n    }\n  }\n<\/style>\n\n<div class=\"sticky-bar\" id=\"stickyBar\">\n  <div class=\"sticky-bar-container\">\n    <div class=\"sticky-bar-left\">\n      <img src=\"{{ product.featured_image | img_url: 'medium' }}\" alt=\"{{ product.title }}\" class=\"product-image\" \/>\n      <div class=\"sticky-bar-details\">\n        <div class=\"title title-full\">{{ product.title }}<\/div>\n        <div class=\"title title-short\">{{ product.title | split: ' Options' | first }}<\/div>\n        <div class=\"price-section\">\n          <span class=\"price\">{{ product.price | money }}<\/span>\n          {% if product.compare_at_price and product.compare_at_price > product.price %}\n            <span class=\"compare-price\">{{ product.compare_at_price | money }}<\/span>\n          {% endif %}\n        <\/div>\n      <\/div>\n    <\/div>\n    <a href=\"javascript:void(0);\" class=\"add-to-cart-btn\" onclick=\"scrollToTop()\">Add to Cart<\/a>\n  <\/div>\n<\/div>\n\n<script>\n  document.addEventListener('scroll', function () {\n    const scrollThreshold = 100;\n    const stickyBar = document.getElementById('stickyBar');\n\n    if (window.scrollY > scrollThreshold) {\n      stickyBar.classList.add('visible');\n    } else {\n      stickyBar.classList.remove('visible');\n    }\n  });\n\n  function scrollToTop() {\n    window.scrollTo({ top: 0, behavior: 'smooth' });\n  }\n<\/script>","color_scheme":"scheme-1","padding_top":0,"padding_bottom":0}},"rich_text_j999Vi":{"type":"rich-text","blocks":{"heading_FNYhDF":{"type":"heading","settings":{"heading":"Talk about your brand","heading_size":"h1"}},"text_gXhGbn":{"type":"text","settings":{"text":"<p>Share information about your brand with your customers. Describe a product, make announcements, or welcome customers to your store.<\/p>"}},"button_nDBNzk":{"type":"button","settings":{"button_label":"Button label","button_link":"shopify:\/\/products\/custom-section","button_style_secondary":false,"button_label_2":"","button_link_2":"","button_style_secondary_2":false}}},"block_order":["heading_FNYhDF","text_gXhGbn","button_nDBNzk"],"settings":{"desktop_content_position":"center","content_alignment":"center","color_scheme":"scheme-4","full_width":true,"padding_top":40,"padding_bottom":52}},"multicolumn_7xNhhC":{"type":"multicolumn","blocks":{"column_gRpNGQ":{"type":"column","settings":{"title":"Column","text":"<p>Pair text with an image to focus on your chosen product, collection, or blog post. Add details on availability, style, or even provide a review.<\/p>","link_label":"","link":""}},"column_wEUECh":{"type":"column","settings":{"title":"Column","text":"<p>Pair text with an image to focus on your chosen product, collection, or blog post. Add details on availability, style, or even provide a review.<\/p>","link_label":"","link":""}},"column_TLxb89":{"type":"column","settings":{"title":"Column","text":"<p>Pair text with an image to focus on your chosen product, collection, or blog post. Add details on availability, style, or even provide a review.<\/p>","link_label":"","link":""}}},"block_order":["column_gRpNGQ","column_wEUECh","column_TLxb89"],"settings":{"title":"Multicolumn","heading_size":"h1","image_width":"full","image_ratio":"adapt","columns_desktop":3,"column_alignment":"left","background_style":"prim